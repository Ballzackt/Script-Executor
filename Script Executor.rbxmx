-- Services
local ScriptEditorService = game:GetService("ScriptEditorService")
local ServerScriptService = game:GetService("ServerScriptService")
local UserInputService = game:GetService("UserInputService")
local StudioService = game:GetService("StudioService")
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local Selection = game:GetService("Selection")

-- References 
local Folder = script.Parent
local Storage = Folder:WaitForChild("Storage")
local ScriptExecutorFrame = Folder:WaitForChild("ScriptExecutorFrame")
local CodeGroup = ScriptExecutorFrame:WaitForChild("CodeGroup")
local CodeText = CodeGroup:WaitForChild("CodeFrame"):WaitForChild("CodeText")
local CodeButtons = ScriptExecutorFrame:WaitForChild("Buttons")

-- Variables
local fileState = nil 
local PluginId = 117739697778479

-- Create Toolbar
local Toolbar = plugin:CreateToolbar("Script Executor")
local ToolbarButtons = {
	ScriptExecutor = Toolbar:CreateButton("Script Executor", "Lauch the script executor.", "rbxassetid://72336447432234"),
	AdvancedEditor = Toolbar:CreateButton("Advanced Script Editor", "Code in Studio's editor.", "rbxassetid://124640403494387")
}

-- Create Widget
local widgetInfo = DockWidgetPluginGuiInfo.new(
	Enum.InitialDockState.Float,
	false,
	false,
	500,
	300 + 24,
	500,
	300 + 24
)
local widget = plugin:CreateDockWidgetPluginGui("ScriptExecutor", widgetInfo)
widget.Title = "Script Executor"
widget.Name = "ScriptExecutorGui"
ScriptExecutorFrame.Parent = widget

-- Toolbar Functions
ToolbarButtons.ScriptExecutor.Click:Connect(function()
	widget.Enabled = not widget.Enabled
	ToolbarButtons.ScriptExecutor:SetActive(widget.Enabled)
end)
widget:BindToClose(function()
	ToolbarButtons.ScriptExecutor:SetActive(false)
	widget.Enabled = false
end)

-- Plugin Functions
local function executeCode(code: string)
	if RunService:IsRunning() then
		-- If playing the game, real scripts will be run
		local exampleScript = Instance.new("Script")
		if RunService:IsServer() then
			exampleScript.RunContext = Enum.RunContext.Server
		else
			exampleScript.RunContext = Enum.RunContext.Client
		end
		exampleScript.Source = code
		exampleScript.Parent = game
		exampleScript.Name = "InjectedScript"
	else
		-- If in the editor, a module script will be run
		local NewScript = Instance.new("ModuleScript")
		NewScript.Name = "InjectedScript"
		NewScript.Source = code .. "\nreturn nil"
		NewScript.Parent = Folder.Storage
		require(NewScript)
		NewScript:Destroy()
	end
end

-- Create the Script or the Advanced Editor
local advancedEditorScript = Instance.new("Script")
advancedEditorScript.Name = "AdvancedEditorScript"
advancedEditorScript.Parent = Storage
local hasAdvancedEditorOpened = false

ToolbarButtons.AdvancedEditor.Click:Connect(function()
	local currentSelection = game.Selection:Get()
	plugin:OpenScript(advancedEditorScript)
end)

-- Advanced Editor Functions
RunService.RenderStepped:Connect(function()
	-- Check if the Advanced Aditor is opened
	local ScriptDocuments = ScriptEditorService:GetScriptDocuments()
	hasAdvancedEditorOpened = false
	for i, v in pairs(ScriptDocuments) do
		if string.find(v.Name, "Storage.AdvancedEditorScript") then
			hasAdvancedEditorOpened = true
		end
	end
	-- Prevent scripting on the widget if the Advanced Editor is opened
	CodeText.Interactable = not hasAdvancedEditorOpened
	-- Set the widget title
	if hasAdvancedEditorOpened then
		widget.Title = "Close Advanced Editor to edit on this window."
	else
		if fileState == "Import" then
			widget.Title = "Script Executor - Importing"
		elseif fileState == "Export" then
			widget.Title = "Script Executor - Exporting"
		else
			widget.Title = "Script Executor"
		end
	end
	
	-- Only allow execution while in the Advanced Editor
	for i, v in pairs(CodeButtons:GetChildren()) do
		if v:IsA("TextButton") then
			if v.Name ~= "Execute" and v.Name ~= "Export" then
				v.Visible = not hasAdvancedEditorOpened
			end
			if v.Name == "Exit" then
				v.Visible = hasAdvancedEditorOpened
			end
		end
	end
	if hasAdvancedEditorOpened then
		CodeButtons.UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Left
		CodeButtons.Size = UDim2.new(1, -20,0, 40)
	else
		CodeButtons.UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
		CodeButtons.Size = UDim2.new(1, 0,0, 40)
	end
end)

-- Advanced editor functions
advancedEditorScript.Source = CodeText.Text		
CodeText:GetPropertyChangedSignal("Text"):Connect(function()
	if not hasAdvancedEditorOpened then
		advancedEditorScript.Source = CodeText.Text		
	end
end)
advancedEditorScript:GetPropertyChangedSignal("Source"):Connect(function()
	CodeText.Text = advancedEditorScript.Source
end)

-- Buttons
CodeButtons:WaitForChild("Execute").MouseButton1Down:Connect(function()
	executeCode(CodeText.Text)
end)
CodeButtons:WaitForChild("Clear").MouseButton1Down:Connect(function()
	-- Clears the text box
	CodeText.Text = ""
end)
CodeButtons:WaitForChild("Import").MouseButton1Down:Connect(function()
	-- Imports a .lua file to the text box
	fileState = "Import"

	local file = StudioService:PromptImportFile({"lua"})

	if file then
		local fileContents = file:GetBinaryContents()

		local luaCode = tostring(fileContents)

		CodeText.Text = luaCode
		print("Successfully imported "..file.Name)
	else
		print("Failed to import. No file selected.")
	end
	
	fileState = nil
end)

CodeButtons:WaitForChild("Export").MouseButton1Down:Connect(function()
	-- Makes a script then force-selects and saves it
	fileState = "Export"

	local newScript = Instance.new("Script")
	local scriptParent = Instance.new("Folder")
	scriptParent.Name = "ScriptExecutorStorage"
	scriptParent.Parent = game.ReplicatedStorage

	newScript.Source = CodeText.Text
	newScript.Parent = scriptParent
	
	local currentSelection = game.Selection:Get()
	game.Selection:Set({newScript})
	
	plugin:PromptSaveSelection("Script")

	scriptParent:Destroy()
	game.Selection:Set(currentSelection) -- Returns to original selection

	fileState = nil
end)

CodeButtons.Exit.MouseButton1Down:Connect(function()
	local ScriptDocuments = ScriptEditorService:GetScriptDocuments()
	for i, v in pairs(ScriptDocuments) do
		if string.find(v.Name, "Storage.AdvancedEditorScript") then
			v:CloseAsync()
		end
	end
end)

-- Button Effects
for i, v in pairs(CodeButtons:GetChildren()) do
	if v:IsA("TextButton") then

		local normalSizeX = v.Size.X.Offset
		local hoverSizeX = v.Size.X.Offset + 10
		local normalColor = Color3.new(0.27451, 0.27451, 0.27451)
		local hoverColor = Color3.new(0.117647, 0.521569, 0.772549)
		if v.Name == "Execute" then
			hoverColor = Color3.new(0.12549, 0.654902, 0.25098)
		end
		if v.Name == "Clear" then
			hoverColor = Color3.new(0.811765, 0.180392, 0.192157)
		end
		if v.Name == "Import" then
			hoverColor = Color3.new(0.0431373, 0.352941, 0.686275)
		end
		if v.Name == "Export" then
			hoverColor = Color3.new(0.0431373, 0.352941, 0.686275)
		end
		if v.Name == "Exit" then
			hoverColor = Color3.new(0.8, 0.572549, 0)
		end
		local hoverStartSizeTween = TweenService:Create(v, TweenInfo.new(0.5, Enum.EasingStyle.Back), {Size = UDim2.fromOffset(hoverSizeX, v.Size.Y.Offset)})
		local hoverStartColorTween = TweenService:Create(v, TweenInfo.new(0.5, Enum.EasingStyle.Sine), {BackgroundColor3 = hoverColor})
		local hoverEndSizeTween = TweenService:Create(v, TweenInfo.new(0.5, Enum.EasingStyle.Back), {Size = UDim2.fromOffset(normalSizeX, v.Size.Y.Offset)})
		local hoverEndColorTween = TweenService:Create(v, TweenInfo.new(0.5, Enum.EasingStyle.Sine), {BackgroundColor3 = normalColor})
		v.MouseEnter:Connect(function()
			hoverStartSizeTween:Play()
			hoverStartColorTween:Play()
		end)
		v.MouseLeave:Connect(function()
			hoverEndSizeTween:Play()
			hoverEndColorTween:Play()
		end)
	end
end

print("Script Executor successfully loaded!")]]></ProtectedString>
